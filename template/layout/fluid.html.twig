{# Plantilla fluida con menu lateral#}
{% import 'macro-formularios.html.twig' as form %}
<!DOCTYPE html>
<html lang="es">
    <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{ constant("TITULO")}}">
    <meta name="author" content="Edgar Messia (pronet21.com)">
    {% block head %}
    <!-- bootstrap & fontawesome -->
    <link rel="stylesheet" href="{{ constant("ROOT")}}/css/bootstrap.min.css" />
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" />

    <!-- text fonts -->
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:400,300" />

    <!-- ace styles -->
    <link rel="stylesheet" href="{{ constant("ROOT")}}/css/ace.min.css" id="main-ace-style" />

    <!--[if lte IE 9]>
            <link rel="stylesheet" href="assets/css/ace-part2.min.css" />
    <![endif]-->
    <link rel="stylesheet" href="{{ constant("ROOT")}}/css/ace-skins.min.css" />
    <link rel="stylesheet" href="{{ constant("ROOT")}}/css/ace-rtl.min.css" />

    <!-- basic scripts -->

    <!--[if !IE]> -->
    <script src="{{ constant("ROOT")}}/js/jquery-1.10.2.min.js"></script>

    <!-- <![endif]-->

    <!--[if IE]>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <![endif]-->

    <!--[if !IE]> -->
    <script type="text/javascript">
            window.jQuery || document.write("<script src='assets/js/jquery.min.js'>"+"<"+"/script>");
    </script>

    <!-- <![endif]-->

    <!--[if IE]>
    <script type="text/javascript">
     window.jQuery || document.write("<script src='assets/js/jquery1x.min.js'>"+"<"+"/script>");
    </script>
    <![endif]-->
        <!--[if lte IE 9]>
          <link rel="stylesheet" href="{{ constant("ROOT")}}/css/ace-ie.min.css" />
        <![endif]-->

        <!-- inline styles related to this page -->

        <!-- ace settings handler -->
        <script src="{{ constant("ROOT")}}/js/ace-extra.min.js"></script>

        <!-- HTML5shiv and Respond.js for IE8 to support HTML5 elements and media queries -->

        <!--[if lte IE 8]>
        <script src="{{ constant("ROOT")}}/js/html5shiv.min.js"></script>
        <script src="{{ constant("ROOT")}}/js/respond.min.js"></script>
        <![endif]-->
        
        <link rel="shortcut icon" href="{{ constant("ROOT")}}/images/favicon.png">
        
        {% endblock %}
        <title>{% block titulo %}{% endblock %} - {{ constant('TITULO')}}</title>
        
    </head>
    <body class="no-skin">
        {% include 'misc/navbar.html.twig' %}
        <div id="main-container" class="main-container">
	<script type="text/javascript">
            try{ace.settings.check('main-container' , 'fixed')}catch(e){}
	</script>
        {% include 'misc/sidebar.html.twig' %}
        <div class="main-content">

        {% block contenido %}
        {% endblock %}

        </div>
        <div class="footer">
        {% block footer %}
        <div class="footer-inner">
        <div class="footer-content">
        <span class="bigger-120">
        <h4 id="id-company-text" class="blue">Gestionado con <span class="red">Valoriza</span><span class="green"><sup><strong>2</strong></sup></span>
        &nbsp; &nbsp;
        <span class="action-buttons">
        <a href="#">
        <i class="ace-icon fa fa-twitter-square light-blue bigger-150"></i>
        </a>
        <a href="#">
        <i class="ace-icon fa fa-facebook-square text-primary bigger-150"></i>
        </a>
        <a href="#">
        <i class="ace-icon fa fa-rss-square orange bigger-150"></i>
        </a>
        </span>
        </h4>
        </div>
        </div>
        {% endblock %}
        </div>
        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
        <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
        </a>
        </div> 
            <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script type="text/javascript">
            if('ontouchstart' in document.documentElement) document.write("<script src='assets/js/jquery.mobile.custom.min.js'>"+"<"+"/script>");
    </script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>

    <!-- page specific plugin scripts -->

    <!--[if lte IE 8]>
      <script src="{{ constant("ROOT")}}/js/excanvas.min.js"></script>
    <![endif]-->
    <script src="{{ constant("ROOT")}}/js/jquery-ui.custom.min.js"></script>
    <script src="{{ constant("ROOT")}}/js/jquery.ui.touch-punch.min.js"></script>
    <script src="{{ constant("ROOT")}}/js/jquery.easypiechart.min.js"></script>
    <script src="{{ constant("ROOT")}}/js/jquery.sparkline.min.js"></script>
    <!-- ace scripts -->
    <script src="{{ constant("ROOT")}}/js/ace-elements.min.js"></script>
    <script src="{{ constant("ROOT")}}/js/ace.min.js"></script>
    </body>
</html>
